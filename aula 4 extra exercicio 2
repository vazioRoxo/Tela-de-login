import java.util.Scanner;

class Lista2 {
  public static void main(String[] args) {
     double gasolina,etanol;
		Scanner ler = new 
    Scanner(System.in);
    
    System.out.println("informe o preço da gasolina:");
	gasolina = ler.nextDouble();
	
	System.out.println("Informe o preço do etanol:");
	etanol = ler.nextDouble();
    
    if (gasolina == etanol){
    	System.out.println("Os os valores são iguas:");
	
	}else{
			
			System.out.println("O valor mais em conta é:"+Math.min (gasolina, etanol));
  }
  ler.close();
  }
}
