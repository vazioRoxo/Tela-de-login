import java.util.Scanner;

class Main {
  public static void main(String[] args) {
     double num1,num2;
		Scanner ler = new 
    Scanner(System.in);
    
    System.out.println("informe o numero primeiro numero:");
	num1 = ler.nextDouble();
	
	System.out.println("Informe o segundo numero:");
	num2 = ler.nextDouble();
    
    if (num1 == num2){
    	System.out.println("Os numeros são iguais:");
	
	}else{
			
			System.out.println("O maior numero é:"+Math.max (num1, num2));
			System.out.println("O menor numero é:"+Math.min (num1, num2));
  }
  ler.close();
  }
}
